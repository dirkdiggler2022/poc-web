{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Certificates": {
      "Default": {
        "Path": "C:/Temp/CertChain/certificate.pfx",
        "Password": "password"
      }
    }
  },
  "Tunnel": {
    //https addres of public frontend
    "Url": "https://192.168.1.16:7243/connect-ws/Agent1"
  },
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "{**catch-all}"
        }
      },
      "route2": {
        "ClusterId": "cluster2",
        "Match": {
          "Path": "/Health/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "cluster1/destination1": {
            //the customer website address
            "Address": "https://localhost:7067"
          }
        }
      },
      "cluster2": {
        "Destinations": {
          "cluster2/destination1": {
            //the customer website address
            "Address": "https://192.168.1.16:7243"
          }
        },
        "HealthCheck": {
          "Active": {
            "Enabled": "true",
            "Interval": "00:00:10",
            "Timeout": "00:00:10",
            "Policy": "ConsecutiveFailures",
            "Path": "/api/health/Agent1"
          }
        }
      }
    }
  }
}
