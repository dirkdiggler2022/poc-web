{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {

    "Certificates": {
      "Default": {
        "Subject": "localhost",
        "AllowInvalid": true
      }
    }
  },
  "ReverseProxy": {
    "Routes": {
      "thycotic": {
        "ClusterId": "thycotic",
        "Match": {
          "Path": "{**catch-all}",
          "Hosts": [ "thycotic.ihawu.space" ]
        }

      },
      "proxypoc": {
        "ClusterId": "proxypoc",
        "Match": {
          "Path": "{**catch-all}",
          "Hosts": [ "local.proxypoc" ]
        }

      },
      "route2": {
        "ClusterId": "beta",
        "Match": {
          "Path": "/example"
        }
      }
    },
    "Clusters": {
      "thycotic": {
        "Destinations": {
          "alpha/destination1": {
            //must be http
            "Address": "http://thycotic.ihawu.space.app"
          }
        }
      },
      "proxypoc": {
        "Destinations": {
          "alpha/destination1": {
            //must be http
            "Address": "http://local.proxypoc.app"
          }
        }
      },
      "beta": {
        "Destinations": {
          "beta/destination1": {
            "Address": "https://example.com"
          }
        }
      }
    }
  }
}
