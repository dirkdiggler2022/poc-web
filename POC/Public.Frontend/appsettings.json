{
  //"Urls": "http://*:8000;https://*:8001",  //"Urls": "http://*:8000;https://*:8001",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  //"Kestrel": {

  //  "Certificates": {
  //    "Default": {
  //      "Subject": "localhost",
  //      "AllowInvalid": true
  //    }
  //  }
  //},
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "alpha",
        "Match": {
          "Path": "{**catch-all}",
          "Headers": [
            {
              "Name": "host-param",
              "Values": [ "Agent1" ]
            }
          ]
        }

      },
      "route2": {
        "ClusterId": "alpha",
        "Match": {
          "Path": "{**catch-all}",
          "Headers": [
            {
              "Name": "host-param",
              "Values": [ "Agent2" ]
            }
          ]
        }

      }
    },
    "Clusters": {
      "alpha": {
        "Destinations": {
          "alpha/destination1": {
            //must be http
            "Address": "http://backend1.app/"
          }
        }
      },
      "beta": {
        "Destinations": {
          "beta/destination1": {
            "Address": "http://example.com"
          }
        }
      }
    }
  }
}
