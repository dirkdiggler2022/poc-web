{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "alpha",
        "Match": {
          "Path": "{**catch-all}"
        }
      },
      "route2": {
        "ClusterId": "beta",
        "Match": {
          "Path": "/example"
        }
      }
    },
    "Clusters": {
      "alpha": {
        "Destinations": {
          "alpha/destination1": {
            "Address": "https://localhost:7192"
          }
        }
      },
      "beta": {
        "Destinations": {
          "beta/destination1": {
            "Address": "https://backend1.app"
          }
        }
      }
    }
  }
}
