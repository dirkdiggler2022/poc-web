{
  //"Urls": "http://*:8000;https://*:8001",  //"Urls": "http://*:8000;https://*:8001",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Certificates": {
      "Default": {
        "Path": "C:/Temp/CertChain/certificate.pfx",
        "Password": "password"
      }
    }
  },
  //Routes will be loaded runtime see program for implementation
  //they will look similar to below except in memory

  "ReverseProxy": {
    "Routes": {
      "agent1-route": {
        "ClusterId": "agent1-cluster",
        "Match": {
          "Path": "{**catch-all}"
        }

      }
    },
    "Clusters": {
      "agent1-cluster": {
        "Destinations": {
          "agent1/destination1": {
            //must be http
            "Address": "http://proxy.agent1.app/"
          }
        }
      }
    }
  }
}