{
  //"Urls": "http://*:8000;https://*:8001",  //"Urls": "http://*:8000;https://*:8001",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  //"Kestrel": {

  //  "Certificates": {
  //    "Default": {
  //      "Subject": "localhost",
  //      "AllowInvalid": true
  //    }
  //  }
  //},
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "alpha",
        "Match": {
          "Path": "Proxy1/{agent}/{**catch-all}"
        }
      },
      "route2": {
        "ClusterId": "beta",
        "Match": {
          "Path": "Proxy2/{agent}/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "alpha": {
        "Destinations": {
          "alpha/destination1": {
            //must be http
            "Address": "http://agent1.backend1.app/"
          }
        }
      },
      "beta": {
        "Destinations": {
          "beta/destination1": {
            "Address": "https://example.com"
          }
        }
      }
    }
  }
}
